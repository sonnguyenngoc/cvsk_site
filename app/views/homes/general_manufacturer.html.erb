                        <a class="read-more-red font-medium" href="#">Xem tiếp</a>
                        <div class="clearfix"></div>
                    </div>
                </div>

                <!-- Widget Container 2 -->
                <div class="col-md-4 col-sm-6">
                    <div class="widget widget-menu-highlights">
                        <h3 class="font-large">Món ăn  nổi bật</h3>
                        <ul>
                            <li>
                                <div class="image"><img src="<%= url_for(" /assets/frontend/showcase/56x56.png ") %>" width="56" height="56" alt=""></div>
                                <h4 class="title"><a href="" title="">French Wedge</a></h4>
                                <div class="excerpt">
                                    <p>Suspendisse dictu m suscipit odio. Nulla dui arcu, fermentum quis, mattis quis, ultricies. </p>
                                </div>
                                <div class="clearfix"></div>
                            </li>
                            <li>
                                <div class="image"><img src="<%= url_for(" /assets/frontend/showcase/56x56.png ") %>" width="56" height="56" alt=""></div>
                                <h4 class="title"><a href="" title="">Tarte aux pommes</a></h4>
                                <div class="excerpt">
                                    <p>Suspendisse dictu m suscipit odio. Nulla dui arcu, fermentum quis, mattis quis, ultricies. </p>
                                </div>
                                <div class="clearfix"></div>
                            </li>
                        </ul>
                        <a class="read-more-red font-medium" href="#">Xem tiếp</a>
                        <div class="clearfix"></div>
                    </div>
                </div>

                <!-- Widget Container 3 -->
                <div class="col-md-4 col-sm-6">
                    <div class="widget widget-contact-us">
                        <h3 class="font-large">Đăng ký nhận tin mới</h3>
                        <form name="contact-us" method="post" class="contact-us-form">
                            <ul>
                                <li>
                                    <input class="form-control float-label" placeholder="Nhập email của bạn" type="text" name="email" title="Please enter your email address" />
                                </li>
                                <li><a href="#" role="button" class="tl-button dark submit font-medium">Đăng ký</a></li>
                            </ul>
                            <input type="hidden" name="handler" value="contact" />
                        </form>
                        <div class="clearfix"></div>
                    </div>
                </div>

            </div>
            <!-- .row -->
        </div>
        <!-- .container -->
    </div>
    <!-- #prefooter-wrapper -->
</div>
<!-- #main-container -->